{
  "_meta": {
    "title": "Visual FaQtory â€” Safe Video2Video Workflow",
    "version": "v0.1.1-alpha",
    "description": "LOW-RISK V2V using LoadVideo + KSampler (low denoise) + VHS_VideoCombine. No experimental nodes.",
    "required_nodes": ["LoadVideo", "CheckpointLoaderSimple", "CLIPTextEncode", "VAEEncode", "KSampler", "VAEDecode", "VHS_VideoCombine"]
  },
  "1": {
    "class_type": "VHS_LoadVideo",
    "inputs": {
      "video": "precond_video.mp4",
      "force_rate": 8,
      "force_size": "Disabled",
      "frame_load_cap": 32,
      "skip_first_frames": 0,
      "select_every_nth": 1
    }
  },
  "2": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "sd15.safetensors"
    }
  },
  "3": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "POSITIVE_PROMPT_PLACEHOLDER",
      "clip": ["2", 1]
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "NEGATIVE_PROMPT_PLACEHOLDER",
      "clip": ["2", 1]
    }
  },
  "5": {
    "class_type": "VAEEncode",
    "inputs": {
      "pixels": ["1", 0],
      "vae": ["2", 2]
    }
  },
  "6": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 42,
      "steps": 20,
      "cfg": 4.5,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 0.35,
      "model": ["2", 0],
      "positive": ["3", 0],
      "negative": ["4", 0],
      "latent_image": ["5", 0]
    }
  },
  "7": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["6", 0],
      "vae": ["2", 2]
    }
  },
  "8": {
    "class_type": "VHS_VideoCombine",
    "inputs": {
      "frame_rate": 8,
      "loop_count": 0,
      "filename_prefix": "vfaq_v2v",
      "format": "video/h264-mp4",
      "images": ["7", 0],
      "save_output": true,
      "pingpong": false
    }
  }
}
