{
  "_meta": {
    "title": "Visual FaQtory - Longcat SVD Continuation",
    "version": "v0.3.5-beta",
    "description": "True autoregressive continuation: loads tail frames from previous output via VHS_LoadVideo, extracts the LAST frame as SVD conditioning image via GetImageFromBatch, generates NEW frames via SVD_img2vid_Conditioning + KSampler temporal diffusion, encodes output via VHS_VideoCombine. The generated frames are NEW timeline content, NOT restyled context. Checkpoint and frame counts are injected dynamically by the longcat backend."
  },
  "1": {
    "class_type": "VHS_LoadVideo",
    "inputs": {
      "video": "CONTEXT_VIDEO_PATH",
      "force_rate": 8,
      "force_size": "Disabled",
      "frame_load_cap": 16,
      "skip_first_frames": 0,
      "select_every_nth": 1
    }
  },
  "2": {
    "class_type": "GetImageFromBatch",
    "inputs": {
      "images": [
        "1",
        0
      ],
      "index": -1
    }
  },
  "3": {
    "class_type": "ImageOnlyCheckpointLoader",
    "inputs": {
      "ckpt_name": "svd_xt.safetensors"
    }
  },
  "4": {
    "class_type": "SVD_img2vid_Conditioning",
    "inputs": {
      "width": 1024,
      "height": 576,
      "video_frames": 16,
      "motion_bucket_id": 127,
      "fps": 8,
      "augmentation_level": 0.02,
      "clip_vision": [
        "3",
        1
      ],
      "init_image": [
        "2",
        0
      ],
      "vae": [
        "3",
        2
      ]
    }
  },
  "5": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 1337,
      "steps": 20,
      "cfg": 2.5,
      "sampler_name": "euler",
      "scheduler": "karras",
      "denoise": 1.0,
      "model": [
        "3",
        0
      ],
      "positive": [
        "4",
        0
      ],
      "negative": [
        "4",
        1
      ],
      "latent_image": [
        "4",
        2
      ]
    }
  },
  "6": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": [
        "5",
        0
      ],
      "vae": [
        "3",
        2
      ]
    }
  },
  "7": {
    "class_type": "VHS_VideoCombine",
    "inputs": {
      "images": [
        "6",
        0
      ],
      "frame_rate": 8,
      "loop_count": 0,
      "filename_prefix": "longcat_continuation",
      "format": "video/h264-mp4",
      "pingpong": false,
      "save_output": true
    }
  }
}